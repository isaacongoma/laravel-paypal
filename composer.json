    "license": "MIT",
    "authors": [
        {
            "name": "Isaac Ongoma",
            "email": "isaacshisia@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "6.*",
        "ext-curl": "*",
        "ext-json": "*",
        "paypal/rest-api-sdk-php": "dev-master"
    },
    "require-dev":{
        "phpspec/phpspec": "~2.0"
    },
    "autoload": {
        "psr-0": {
